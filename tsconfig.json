{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/**"
  ],
  "typeAcquisition": {
    "enable": true,
    "exclude": ["node_modules"]
  },
  "extends": "expo/tsconfig.base"
}
